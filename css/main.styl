@import 'mixins.styl'

*
	bbox()

body, html
	rect 100%
	margin 0
	padding 0
	overflow auto
	font-family monospace

.touch
	overflow hidden !important
	border 10px solid red

#wrapper
	position absolute
	left 0
	top 0
	rect 100%
	overflow auto
	
	.buttons-l
		padding 125px 0 250px 0
		background: rgb(105,210,231); /* Old browsers */
		/* IE9 SVG, needs conditional override of 'filter' to 'none' */
		// background: url(data:image/svg+xml;base64,);
		background: -moz-linear-gradient(top,  rgba(105,210,231,1) 0%, rgba(105,210,231,1) 50%, rgba(254,67,101,1) 51%, rgba(254,67,101,1) 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(105,210,231,1)), color-stop(50%,rgba(105,210,231,1)), color-stop(51%,rgba(254,67,101,1)), color-stop(100%,rgba(254,67,101,1))); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(top,  rgba(105,210,231,1) 0%,rgba(105,210,231,1) 50%,rgba(254,67,101,1) 51%,rgba(254,67,101,1) 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(top,  rgba(105,210,231,1) 0%,rgba(105,210,231,1) 50%,rgba(254,67,101,1) 51%,rgba(254,67,101,1) 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(top,  rgba(105,210,231,1) 0%,rgba(105,210,231,1) 50%,rgba(254,67,101,1) 51%,rgba(254,67,101,1) 100%); /* IE10+ */
		background: linear-gradient(to bottom,  rgba(105,210,231,1) 0%,rgba(105,210,231,1) 50%,rgba(254,67,101,1) 51%,rgba(254,67,101,1) 100%); /* W3C */
		// filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#69d2e7', endColorstr='#fe4365',GradientType=0 ); /* IE6-8 */
		

.section-l
	height 300px
	margin-bottom -125px




	

	



